<!--index.wxml-->
<!-- 搜索框 -->
 <navigator url="/pages/search/search" hover-class="changestyle">
    <view class="view-search">
    
        <input class="input" placeholder-class="input-placeholder" placeholder="输入电脑和配置" bindinput="bindSearchInput" />
        <image class="button" src="/images/search.png" bindtap="tapSearch" />
  
    </view>
   </navigator>

<!-- tab界面 -->
<view class="swiper-tab">
  <view class="swiper-tab-item {{currentTab==0?'active':''}}" data-current="0" bindtap="clickTab">cpu</view>
  <view class="swiper-tab-item {{currentTab==1?'active':''}}" data-current="1" bindtap="clickTab">显卡</view>
  <view class="swiper-tab-item {{currentTab==2?'active':''}}" data-current="2" bindtap="clickTab">主板</view>
  <view class="swiper-tab-item {{currentTab==3?'active':''}}" data-current="3" bindtap="clickTab">显示器</view>
 
</view>

<swiper current="{{currentTab}}" duration="300" bindchange="swiperTab">
