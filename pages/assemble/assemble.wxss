/**index.wxss**/

.itemindex{
    width: 100%;
    height: 220rpx;
    position: relative; 
    display: flex;
    margin:10rpx 10rpx;
    border-bottom: 1px solid rgb(197, 199, 199);
}
.itemindex:first-child{
   margin-top: 0;
}
.itemindex .remove{
    width: 60px;
    height: 100%;
    background-color: red;
    position: absolute;
    top: 0;
    right: -60px;
    display: flex;
    justify-content: center;
    align-items: center;
}
.itemindex .number-wrapper{
    height: 100%;
    flex: 1;
    display: flex;
    flex-direction: column;
    /* justify-content: space-between; */
}
.itemindex .ok{
    width: 60px;
    height: 100%;
    padding-right: 20rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    background-color: #98F5FF;
}
.itemindex .img{
    width: 150rpx;
    height:150rpx;
    padding: 20rpx;
}
.number-wrapper .name{
    margin: 10rpx 10rpx 10rpx 10rpx;
    font-size: 39rpx;
    overflow: hidden;
   
   
}
.number-wrapper .count-wrapper{
  display: flex;
  align-items: center;
  margin-left: 10rpx;
   font-size: 25rpx;
}
.number-wrapper .count-wrapper .decrease-btn{
  font-size: 30rpx;
}
.number-wrapper .count-wrapper .increase-btn{
    font-size: 30rpx;
}
.number-wrapper .count-wrapper .count{
  margin: 0 1rpx 0 1rpx;
  font-size: 30rpx;
}
.number-wrapper .price-wrapper{
    margin-left: 10rpx;
}

.number-wrapper .price-wrapper .price{
  margin-left: 10rpx;
  font-size: 30rpx;
  color: rgb(252, 83, 5);
}
.number-wrapper .price-wrapper .people{
  margin-left: 250rpx;
  font-size: 30rpx;
  
}
